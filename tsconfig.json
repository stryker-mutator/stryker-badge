        "module": "commonjs",
        "forceConsistentCasingInFileNames": true,
        "lib": [
            "es5",
            "es2015"
        ],
        "noImplicitReturns": true,
        "noUnusedLocals": true,
        "sourceMap": true,
        "strict": true,
        "target": "es2015",
        "strictPropertyInitialization": false // TODO
    }
}