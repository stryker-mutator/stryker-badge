  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "E2E tests (acceptance)",
      "program": "${workspaceFolder}/../../node_modules/protractor/bin/protractor",
      "cwd": "${workspaceFolder}",
      "args": [
        "${workspaceFolder}/dist/protractor.conf.js"
      ],
      "internalConsoleOptions": "openOnSessionStart"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "E2E tests (local)",
      "program": "${workspaceFolder}/../../node_modules/protractor/bin/protractor",
      "cwd": "${workspaceFolder}",
      "args": [
        "${workspaceFolder}/dist/protractor.conf.js",
        "--baseUrl",
        "http://localhost:1337"
      ],
      "internalConsoleOptions": "openOnSessionStart"
    }
  ]
}