{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "ordered-imports": false,
    "object-literal-sort-keys": true,
    "quotemark": [
      true,
      "single"
    ],
    "trailing-comma": [
      true,
      {
        "multiline": "never",
        "singleline": "never"
      }
    ],
    "eofline": false,
    "no-namespace": false,
    "interface-name": false,
    "max-classes-per-file": false,
    "member-access": false,
    "max-line-length": [
      true,
      150
    ],
    "no-unused-expression": false,
    "arrow-parens": false
  },
  "rulesDirectory": []
}